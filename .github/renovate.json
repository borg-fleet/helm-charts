{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base"
    ],
    "baseBranches": [
        "main"
    ],
    "rebaseWhen": "conflicted",
    "labels": [
        "dependencies"
    ],
    "repositories": [
        "borg-fleet/helm-charts"
    ],
    "automergeStrategy": "merge-commit",
    "prHourlyLimit": 2
}